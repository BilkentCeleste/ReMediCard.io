{
  "expo": {
    "name": "RemediCardMobileApp",
    "slug": "RemediCardMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/remedicon.png",
    "expo-notifications": {
      "icon": "./assets/icons/remedismallicon.png",
      "color": "#2596be"
    },
    "notification": {
      "icon": "./assets/icons/remedismallicon.png",
      "color": "#2596be"
    },
    "scheme": "remedicardio",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/remedicon.png",
        "backgroundColor": "#2596be"
      },
      "package": "io.remedicard.celeste",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/remedismallicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/remedicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2596be"
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/remedismallicon.png",
          "color": "#2596be"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87b48c47-7f1c-4b8f-be51-25d2701b7feb"
      }
    }
  }
}
